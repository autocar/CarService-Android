<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ruhoon.jiayu.merchant"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.ruhoon.jiayu.merchant.application.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.ruhoon.jiayu.merchant.activity.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ruhoon.jiayu.merchant.activity.RetrievePasswordActivity"
            android:label="@string/title_activity_retrieve_password" >
        </activity>
        <activity
            android:name="com.ruhoon.jiayu.merchant.activity.RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name="com.ruhoon.jiayu.merchant.activity.MainActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="com.ruhoon.jiayu.merchant.activity.item.MyItemActivity"
            android:label="@string/title_activity_my_item" >
        </activity>
        <activity
            android:name="com.ruhoon.jiayu.merchant.activity.item.ItemChoiceActivity"
            android:label="@string/title_activity_item_choice" >
        </activity>
        <activity
            android:name="com.ruhoon.jiayu.merchant.activity.TermsOfServiceActivity"
            android:label="@string/title_activity_terms_of_service" >
        </activity>
        <activity
            android:name="com.ruhoon.jiayu.merchant.activity.item.AddItemActivity"
            android:label="@string/title_activity_add_item" >
        </activity>
        <activity
            android:name="com.ruhoon.jiayu.merchant.activity.item.ItemDetailActivity"
            android:label="@string/title_activity_iten_detail" >
        </activity>
        <activity
            android:name="com.ruhoon.jiayu.merchant.activity.getorder.GetOrderActivity"
            android:label="@string/title_activity_get_order" >
        </activity>
        <activity
            android:name="com.ruhoon.jiayu.merchant.activity.order.MyOrderActivity"
            android:label="@string/title_activity_my_order" >
        </activity>
       
    </application>

</manifest>